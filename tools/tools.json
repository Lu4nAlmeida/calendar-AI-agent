[
    {
        "type": "function",
        "name": "create_event",
        "description": "Creates an event on the user's Google Calendar, can also create recurring events.",
        "parameters": {
            "type": "object",
            "description": "The event's body formatted to Google's Event Resource schema, when inputting time use ISO 8601 format (YYYY-MM-DDTHH:MM:SS±HH:MM). The date, in the format \"yyyy-mm-dd\", if this is an all-day event.",
            "properties": {
                "summary": {"type": "string"},
                "description": {"type": "string"},
                "colorId": {"type": "string"},
                "start": {
                    "type": "object",
                    "properties": {
                        "dateTime": {"type": "string"},
                        "timeZone": {"type": "string"}
                    },
                    "required": ["dateTime", "timeZone"]
                },
                "end": {
                    "type": "object",
                    "properties": {
                        "dateTime": {"type": "string"},
                        "timeZone": {"type": "string"}
                    },
                    "required": ["dateTime", "timeZone"]
                },
                "creator": {
                    "type": "object",
                    "properties": {
                        "id": {"type": "string"},
                        "email": {"type": "string"},
                        "displayName": {"type": "string"},
                        "self": {"type": "boolean"}
                    }
                },
                "organizer": {
                    "type": "object",
                    "properties": {
                        "id": {"type": "string"},
                        "email": {"type": "string"},
                        "displayName": {"type": "string"},
                        "self": {"type": "boolean"}
                    }
                },
                "attendees": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {"type": "string"},
                            "email": {"type": "string"},
                            "displayName": {"type": "string"}
                        }
                    }
                },
                "reminders": {
                    "type": "object",
                    "properties": {
                        "useDefault": {"type": "boolean"},
                        "overrides": {

                        }
                    }
                },
                "recurrence": {
                    "type": "array",
                    "description": "RFC 5545 recurrence rules (e.g., 'RRULE:FREQ=WEEKLY;COUNT=10'). Omit for one-time events.",
                    "items": {
                        "type": "string",
                        "description": "Single recurrence rule line, e.g., 'RRULE:FREQ=DAILY;COUNT=5' or 'EXDATE;TZID=Asia/Bangkok:20251225T090000'."
