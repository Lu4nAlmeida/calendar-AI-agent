    {
        "type": "function",
        "name": "list_events",
        "description": "Lists the next 'max_amount' of events on user's calendar. Returns the events IDs, summary, and other useful tags.",
        "parameters": {
            "type": "object",
            "properties": {
                "start_date": {
                    "type": "string",
                    "description": "The lower bound or starting date from which events are going to be listed (already set to current date by default)."
                },
                "end_date": {
                    "type": "string",
                    "description": "The upper bound or ending date to which events are going to be listed (already set to None by default)."
                },
                "max_amount": {
                    "type": "number",
                    "description": "Integer value representing the amount of events to be listed (already set to 20 by default | avoid setting this number to more than 50)."
                },
                "calendarId": {
                    "type": "string",
                    "description": "Calendar to list events from (already set to \"primary\" by default)."
                }
            }
        }
    },
    {
        "type": "function",
        "name": "delete_event",
        "description": "Deletes an event or recurring event on the user's calendar (requires eventId, use 'list_events' to get it)",
        "parameters": {
            "type": "object",
            "properties": {
                "eventId": {
                    "type": "string",
                    "description": "Event identifier."
                }
            },
            "required": ["eventId"]
        }
    },
