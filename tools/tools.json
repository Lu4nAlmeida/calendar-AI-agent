    {
        "type": "function",
        "name": "search_events",
        "description": "Searches for an event within the next 365 days using a keyword. If no exact matches are found, looks for close matches.",
        "parameters": {
            "type": "object",
            "properties": {
                "keyword": {
                    "type": "string",
                    "description": "Searches for an event with this keyword in either the event's summary or description."
                },
                "calendarId": {
                    "type": "string",
                    "description": "Calendar to be searched on (already set to \"primary\" by default)"
                }
            },
            "required": ["keyword"]
        }
    },
    {
        "type": "function",
        "name": "count_days",
        "description": "Counts the amount of days in between two dates.",
        "parameters": {
            "type": "object",
            "properties": {
                "end_date": {
                    "type": "string",
                    "description": "Ending date in standard ISO 8601 format."
                },
                "start_date": {
                    "type": "string",
                    "description": "Starting date in standard ISO 8601 format (already set to current date by default)"
                }
            },
            "required": ["end_date"]
        }
